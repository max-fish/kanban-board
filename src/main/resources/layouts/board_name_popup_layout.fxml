<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import com.jfoenix.controls.JFXDialogLayout?>
<?import com.jfoenix.controls.JFXTextField?>
<?import com.jfoenix.controls.JFXButton?>
<JFXDialogLayout xmlns="http://javafx.com/javafx"
                 xmlns:fx="http://javafx.com/fxml"
                 fx:controller="controllers.BoardNamePopupController"
                 prefHeight="400.0" prefWidth="600.0" stylesheets="@../styling/board_name_popup_styling.css">
    <heading>
        <Label text="Name your board"/>
    </heading>
    <body>
        <JFXTextField fx:id="boardNameTextField" promptText="Board Name"/>
    </body>
    <actions>
        <JFXButton fx:id="okButton" text="Ok"/>
        <JFXButton fx:id="cancelButton" text="Cancel"/>
    </actions>
</JFXDialogLayout>
