<?xml version="1.0" encoding="UTF-8"?>
<?import com.jfoenix.controls.JFXDialogLayout?>
<?import javafx.scene.control.Label?>
<?import com.jfoenix.controls.JFXButton?>
<JFXDialogLayout xmlns="http://javafx.com/javafx"
                 xmlns:fx="http://javafx.com/fxml"
                 fx:controller="controllers.DeleteConfirmationPopupController"
                 prefHeight="400.0" prefWidth="600.0" stylesheets="@../styling/delete_confirmation_popup_styling.css">
    <heading>
        <Label text="Delete Column"/>
    </heading>
    <body>
        <Label text="Are you sure?"/>
    </body>
    <actions>
        <JFXButton fx:id="deleteButton" text="Delete"/>
        <JFXButton fx:id="cancelButton" text="Cancel"/>
    </actions>
</JFXDialogLayout>
