<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controllers.StatisticsController"
            prefHeight="400.0" prefWidth="600.0">

    <Label fx:id="overallVelocity" id="OverallVelocity" maxHeight="Infinity" maxWidth="Infinity" prefHeight="NaN" prefWidth="200"
           text="Overall velocity "/>
    <Label fx:id="leadTime" id="leadTime" maxHeight="Infinity" maxWidth="Infinity" prefHeight="NaN" prefWidth="200"
           text="Lead time"/>
    <Label fx:id="avgWIP" id="avgWIP" maxHeight="Infinity" maxWidth="Infinity" prefHeight="NaN" prefWidth="200"
           text="Average Work In Progress"/>

</AnchorPane>
